{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "outputDirectory": ".next",
    "regions": [
        "iad1"
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "@api-url",
        "GEMINI_API_KEY": "@gemini-api-key"
    },
    "build": {
        "env": {
            "NEXT_PUBLIC_API_URL": "@api-url",
            "GEMINI_API_KEY": "@gemini-api-key"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}